// fibonacci
fun fib(n int) int {
    if (n <= 1) return n
    return fib(n - 1) + fib(n - 2)
}

for i := 0; i < 20; i++ {
    println(fib(5))
}

// sum of a sequence of integers
fun sum(...n int) int {
    acc := 0

    for i := 0; i < len(n); i++ {
        acc += n[i]
    }

    return acc
}

