func buildConfig(ip: string, enabled: bool): struct{
    ip: string
    enabled: bool
} {
    struct Config {
        ip: string
        enabled: bool
    }

    return Config{ enabled: enabled, ip: ip };
}

const ip = buildConfig("192.168.1.1", false).ip;
const testConfig = buildConfig("192.168.1.10", true);

let validIp: string = testConfig.enabled
    ? testConfig.ip
    : buildConfig("192.168.1.10", true).ip;
