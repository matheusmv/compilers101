func makeUser(id: int, username: string, password: string, email: string, street: string, zip: string): User {
    return User{
        id: id,
        username: username,
        password: password,
        email: email,
        address: struct{
            street: string
            zip: string
        }{
            street: street,
            zip: zip
        }
    };
}

